{
  "name": "ani-fit",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "node -e \"process.env.NUXT_SKIP_PREPARE || require('child_process').execSync('nuxt prepare', {stdio: 'inherit'});\"",
    "server": "node ./server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "start": "node .output/server/index.mjs"
  },
  "dependencies": {
    "nuxt": "^3.16.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
